int doUnion(int a[], int n, int b[], int m)  {
        //code here
         set<int>st;  //Set stores element in unique and sorted order.


        for(int i=0;i<n;i++){
            st.insert(a[i]);
        }
        for(int i=0;i<m;i++){
            st.insert(b[i]);
        }
        return st.size();
    
    
    }

